{
  "buildCommand": "npm run clean && npm run build",
  "installCommand": "npm install --force --no-cache --production=false",
  "framework": "nextjs",
  "functions": {
    "app/page.tsx": {
      "maxDuration": 60
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  }
} 